{
  "name": "Play sound",
  "description": "Play audio in service worker-based browser extensions",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "audio.html",
    "open_in_tab": true
  },
  "action": {},
  "author": "guest271314"
}
